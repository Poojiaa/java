import java.util.*;
class perfect
{
 public static void main(String arg[])
{
 int n,i=1,sum=0,m;
 Scanner sc=new Scanner(System.in);
 System.out.println("enter the first number");
 n=sc.nextInt();
m=n;
for(i=1;i<n;i++)
{
 if(n%i==0)
{

sum=sum+i;

}

}
if(sum==m)
{
System.out.println("it is a perfect number");

}
else{
System.out.println("it is not a perfect number");

}
}
}
