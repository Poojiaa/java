import java.util.*;
class  match
{
public static void main(String arg[])
{
String str1,str2;
Scanner sc=new Scanner(System.in);
System.out.println("enter the string");
str1=sc.nextLine();
str2=sc.nextLine();
if(str1.equals(str2))
{
System.out.println("valid match");}
else
{
System.out.println(" not  a valid");
}
}
}
