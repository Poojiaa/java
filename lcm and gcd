import java.util.*;
class R192211732
{
 public static void main(String arg[])
{
 int arr[]={16,20};
 int lcm=arr[0];
 int gcd=arr[0];
 for( int i=1;i<arr.length;i++)
{
gcd=m(arr[i],lcm);
lcm=(lcm*arr[i]/gcd);
}
System.out.println("lcm="+lcm);
System.out.println("gcd="+gcd);

 }
public static int m(int a,int b)
{
if(b==0)
return a;
return m(b,a%b);
}
}
