import java.util.*;
class Numberpattern2
{
public static void main(String arg[])
{
        int rows = 5; 
        
        for (int i = 1; i <= rows; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(j);
            }
           
            System.out.println();
        }
    }
}
