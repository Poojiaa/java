import java.util.*;
class Leapyear
{
 public static void main(String arg[])
{
 int n;
 Scanner sc=new Scanner(System.in);
 System.out.println("enter the year");
 n=sc.nextInt();
  if(n%4==0)
  {
    System.out.println("is a leap year" +n);
  }
 else
 {
    System.out.println(" is not a leap year = "+n);
    
  }

}
}
