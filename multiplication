import java.util.*;
class  Multiplication
{
 public static void main(String arg[])
{
 int num;
 Scanner sc=new Scanner(System.in);
 System.out.println("enter the table");
 num=sc.nextInt();
 for(int i = 1; i <= 10; i++)
        {
            System.out.printf("%d * %d = %d \n", num, i, num * i);
        }
}
}
